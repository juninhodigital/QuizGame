<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="tadsceno01a.g4.com.quizgame" >

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quiz"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeacherActivity"
            android:label="Bem Vindo"
            android:theme="@style/Theme.AppCompat.Light"
            android:parentActivityName="tadsceno01a.g4.com.quizgame.MainActivity" >

        </activity>
    </application>

</manifest>
